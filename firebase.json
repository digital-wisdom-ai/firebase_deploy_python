{
  "functions": [
    {
      "source": "functions",
      "runtime": "python311",
      "codebase": "default",
      "entryPoint": "simple_function",
      "memory": "256MB",
      "timeoutSeconds": 60,
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5001
    },
    "auth": {
      "port": 9099
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
